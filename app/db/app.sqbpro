JOIN teacher_subject ON teacher_subject.teacher_id = users.id 
JOIN teacher_course_type ON teacher_course_type.teacher_id = users.id 
JOIN level ON level.level_id = teacher_level.level_id
JOIN subject ON subject.id = teacher_subject.subject_id
JOIN course_type ON course_type.course_type_id = teacher_course_type.course_type_id
